#!/bin/csh
# Change this next list to run on a different number of nodes
#PBS -N cptest2
#PBS -v ASKAP_ROOT
cd $PBS_O_WORKDIR

setenv AIPSPATH $ASKAP_ROOT/Code/Components/Synthesis/testdata/current

yod -sz all -x AIPSPATH ./csimulator -inputs cptest2.in > cptest2.out.$PBS_JOBID

yod -sz all -x AIPSPATH ./cimager -inputs cptest2.in > cptest2.out.$PBS_JOBID
